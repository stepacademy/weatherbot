<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <system.serviceModel>
    <client>
      <endpoint name ="ServerEndpoint"
                address="http://localhost:12000/Duplex/Server/"
                bindingConfiguration="WSDualHttpBinding_IReceiveActionListenerService"
                binding="wsDualHttpBinding"
                contract="WeatherBot.TeleInteraction.IReceiveActionListenerService" />
    </client>
    <bindings>
      <wsDualHttpBinding>
        <binding name="WSDualHttpBinding_IReceiveActionListenerService"
                 clientBaseAddress="http://localhost:8000/Duplex/Client/" >
          <security mode="None"/>
        </binding>
      </wsDualHttpBinding>
    </bindings>
  </system.serviceModel>
</configuration>