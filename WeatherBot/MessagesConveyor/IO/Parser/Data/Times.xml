<?xml version="1.0" encoding="utf-8" ?>

<TimeData Language="Russian">

    <Times Class="A">
        <Time Subcl="A" Name="позавчера" Tag="Past" Factor="2"/>
        <Time Subcl="B" Name="вчера" Tag="Past" Factor="1"/>
        <Time Subcl="C" Name="сегодня" Tag="Present" Factor="0"/>
        <Time Subcl="D" Name="сейчас" Tag="Present" Factor="0"/>
        <Time Subcl="E" Name="завтра" Tag="Future" Factor="1"/>
        <Time Subcl="F" Name="послезавтра" Tag="Future" Factor="2"/>
